{
  "compilerOptions": {
    "module": "CommonJS",
    "target": "ES2019",
    "moduleResolution": "node",
    "outDir": "./dist/server",
    "esModuleInterop": true,
    "noEmit": false,
    "sourceMap": true,
    "skipLibCheck": true,
    "strict": true
  },
  "include": [
    "server/**/*"
  ],
  "exclude": [
    "node_modules",
    "src"
  ]
}
